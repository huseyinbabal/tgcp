{
  "color_maps": {
    "health_status": [
      { "value": "HEALTHY", "color": [0, 255, 0] },
      { "value": "UNHEALTHY", "color": [255, 0, 0] },
      { "value": "UNKNOWN", "color": [255, 255, 0] }
    ]
  },
  "resources": {
    "forwarding-rules": {
      "display_name": "Forwarding Rules",
      "service": "compute",
      "api": {
        "base": "https://compute.googleapis.com/compute/v1",
        "path": "projects/{project}/global/forwardingRules",
        "method": "GET"
      },
      "response_path": "items",
      "id_field": "id",
      "name_field": "name",
      "columns": [
        { "header": "Name", "json_path": "name", "width": 30 },
        { "header": "IP Address", "json_path": "IPAddress", "width": 16 },
        { "header": "Protocol", "json_path": "IPProtocol", "width": 10 },
        { "header": "Port Range", "json_path": "portRange", "width": 12 },
        { "header": "Target", "json_path": "target", "width": 35 },
        { "header": "Load Balancing", "json_path": "loadBalancingScheme", "width": 15 }
      ],
      "actions": [
        {
          "display_name": "Delete",
          "api": {
            "method": "DELETE",
            "path": "projects/{project}/global/forwardingRules/{name}"
          },
          "shortcut": "ctrl+d",
          "confirm": {
            "message": "Delete forwarding rule '{name}'?",
            "destructive": true
          }
        }
      ]
    },
